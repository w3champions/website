{
  "lineWidth": 999,
  "typescript": {
    "indentWidth": 2,
    "quoteStyle": "preferDouble",
    "newLineKind": "auto",
    "nextControlFlowPosition": "maintain",
    "module.sortImportDeclarations": "maintain"
  },
  "excludes": [
    "**/node_modules",
    "**/package-lock.json",
    "**/*.js",
    "src/locales/data.ts",
    "components.d.ts",
    ".eslintrc.cjs"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.88.10.wasm"
  ]
}
